{
  "name": "checkout-custom",
  "vendor": "vtex",
  "version": "0.0.1",
  "title": "Checkout Custom UI Settings",
  "description": "Make possible to customize the checkout layout using a easy to use interface from the admin",
  "builders": {
    "docs": "0.x",
    "checkout-ui-custom": "0.x",
    "admin": "0.x",
    "react": "3.x",
    "node": "6.x",
    "graphql": "1.x",
    "messages": "1.x"
  },
  "policies": [
    {
      "name": "read-workspace-apps"
    },
    {
      "name": "update-app-settings"
    },
    {
      "name": "outbound-access",
      "attrs": {
        "host": "platform.io.vtex.com",
        "path": "/{{account}}/*"
      }
    }
  ],
  "dependencies": {
    "vtex.styleguide": "9.x"
  },
  "scripts": {},
  "credentialType": "absolute",
  "$schema": "https://raw.githubusercontent.com/vtex/node-vtex-api/master/gen/manifest.schema"
}
